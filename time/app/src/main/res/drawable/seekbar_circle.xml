<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
	   android:shape="oval">
	<!-- solid表示圆的填充色 -->
	<solid android:color="@color/white" />
	<!-- stroke则代表圆的边框线 -->
	<stroke
			android:width="1dp"
			android:color="@color/colorPrimaryDark" />
	<!-- size控制高宽 -->
	<size
			android:height="12dp"
			android:width="12dp" />
</shape>